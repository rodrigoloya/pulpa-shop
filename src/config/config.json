{
  "site": {
    "title": "Pulpa Store",
    "base_url": "https://pulpa.app/",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/logo-pulpa-letras-negras.png",
    "logo_darkmode": "/images/logo_pulpa_letras_blancas.png",
    "logo_width": "150",
    "logo_height": "80",
    "logo_text": "Pulpa.dev"
  },

  "settings": {
    "search": true,
    "account": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system"
  },

  "params": {
    "contact_form_action": "#",
    "copyright": "Designed And Developed by [Themefisher] & Pulpa.dev (https://pulpa.dev/)"
  },

  "navigation_button": {
    "enable": false,
    "label": "Get Started",
    "link": "https://github.com/themefisher/astrofront"
  },

  "metadata": {
    "meta_author": "Pulpa",
    "meta_image": "/images/og-image.png",
    "meta_description": "Pulpa.dev Storefront"
  },

  "shopify": {
    "currencySymbol": "$",
    "currencyCode": "USD",
    "collections": {
      "hero_slider": "hidden-homepage-carousel",
      "featured_products": "featured-products"
    }
  }
}
